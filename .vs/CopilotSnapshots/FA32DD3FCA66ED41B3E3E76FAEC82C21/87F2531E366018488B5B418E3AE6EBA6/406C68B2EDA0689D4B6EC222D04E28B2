using FluentValidation;
using HousingHubBackend.Dtos.Society;

namespace HousingHubBackend.Validators
{
    public class SocietyDtoValidator : AbstractValidator<SocietyDto>
    {
        public SocietyDtoValidator()
        {
            RuleFor(x => x.Name).NotEmpty();
            RuleFor(x => x.Address).NotEmpty();
        }
    }
}
