using AutoMapper;
using HousingHubBackend.Models;
using HousingHubBackend.Dtos.Flat;

namespace HousingHubBackend.Mapping
{
    public class FlatProfile : Profile
    {
        public FlatProfile()
        {
            CreateMap<Flat, FlatDto>().ReverseMap();
            CreateMap<CreateFlatDto, Flat>();
            CreateMap<UpdateFlatDto, Flat>();
        }
    }
}
